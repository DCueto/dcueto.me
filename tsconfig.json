{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@mdxComponents/*": [
        "src/components/shared/astro/mdxComponents/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@i18n/*": [
        "src/i18n/*"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@public/*": [
        "public/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}